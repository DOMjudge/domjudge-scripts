{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "CLICS Contest API - group",
	"description": "Definition of a single group object",

	"type": "object",
	"properties": {
		"id": { "$ref": "common.json#/identifier" },
		"icpc_id": { "type": [ "string", "null" ] },
		"name": { "type": "string" },
		"type": { "type": "string" },
		"hidden": { "type": "boolean" }
	},
	"required": ["id", "name"],
	"additionalProperties": false
}
